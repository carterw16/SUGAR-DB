object capacitor {
	phases ABC;
	name N_800053500_Cap;
	pt_phase ABC;	//pt_phase A;
	parent N_800053500;
	phases_connected ABC;
	control MANUAL;
	voltage_set_high 2520.0;
	voltage_set_low 2280.0;
	capacitor_A 0.10 MVAr;
	capacitor_B 0.10 MVAr;
	capacitor_C 0.10 MVAr;
	control_level BANK; //INDIVIDUAL;
	time_delay 100.0;
	dwell_time 0.0;
	switchA CLOSED;
	switchB CLOSED;
	switchC CLOSED;
	nominal_voltage 2400.00;
}

