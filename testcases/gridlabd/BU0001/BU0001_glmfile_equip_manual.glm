object multi_recorder {
	property N_800053500_Cap:switchA,N_800053500_Cap:switchB,N_800053500_Cap:switchC,N_800003805_Cap:switchA,N_800003805_Cap:switchB,N_800003805_Cap:switchC;
	file "equip_log.csv";
	interval 60;
	limit 2880;
};


